{
    "name": "zvizvi/filament-relation-manager-repeatable",
    "description": "This is my package filament-relation-manager-repeatable",
    "keywords": [
        "zvizvi",
        "laravel",
        "filament-relation-manager-repeatable"
    ],
    "homepage": "https://github.com/zvizvi/filament-relation-manager-repeatable",
    "support": {
        "issues": "https://github.com/zvizvi/filament-relation-manager-repeatable/issues",
        "source": "https://github.com/zvizvi/filament-relation-manager-repeatable"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "zvizvi",
            "email": "zvizvi5@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/tables": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Zvizvi\\FilamentRelationManagerRepeatable\\": "src/",
            "Zvizvi\\FilamentRelationManagerRepeatable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zvizvi\\FilamentRelationManagerRepeatable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zvizvi\\FilamentRelationManagerRepeatable\\FilamentRelationManagerRepeatableServiceProvider"
            ],
            "aliases": {
                "FilamentRelationManagerRepeatable": "Zvizvi\\FilamentRelationManagerRepeatable\\Facades\\FilamentRelationManagerRepeatable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}